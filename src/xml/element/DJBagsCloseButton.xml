<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
    <Button name="DJBagsCloseButton" parentKey="close" inherits="UIPanelCloseButton" virtual="true">
        <Anchors>
            <Anchor point="CENTER" relativePoint="TOPRIGHT" x="-2" y="-2"/>
        </Anchors>
        <NormalTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Disabled" />
        <Scripts>
            <OnLoad>
                self:SetAlpha(0.2)
            </OnLoad>
            <OnEnter>
                self:SetAlpha(1)
            </OnEnter>
            <OnLeave>
                self:SetAlpha(0.2)
            </OnLeave>
        </Scripts>
    </Button>
</Ui>
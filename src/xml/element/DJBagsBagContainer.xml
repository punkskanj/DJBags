<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
    <Frame name="DJBagsBagContainer" frameStrata="MEDIUM" toplevel="true" movable="true" enableMouse="true" hidden="true" parent="UIParent">
        <Backdrop bgFile="Interface\ChatFrame\ChatFrameBackground" edgeFile="Interface\Buttons\WHITE8x8">
            <EdgeSize>
                <AbsValue val="1" />
            </EdgeSize>
            <Color r="0" g="0" b="0" a="0.6" />
            <BorderColor r="0.3" g="0.3" b="0.3" a="1" />
        </Backdrop>
        <Anchors>
            <Anchor point="BOTTOMRIGHT" x="-150" y="150" />
        </Anchors>
        <Frames>
            <Frame name="$parentMainBar" parentKey="mainBar" inherits="DJBagsMainBarTemplate">
                <Anchors>
                    <Anchor point="TOPRIGHT" relativeTo="$parent" relativePoint="BOTTOMRIGHT" x="0" y="-5"/>
                </Anchors>
            </Frame>
            <Button name="$parentClose" parentKey="close" inherits="DJBagsCloseButton" />
        </Frames>
        <Scripts>
            <OnLoad>
                DJBagsBagContainer_OnLoad(self)
            </OnLoad>
            <OnShow>
                DJBagsBagContainer_OnShow(self)
            </OnShow>
            <OnHide>
                DJBagsBagContainer_OnHide(self)
            </OnHide>
        </Scripts>
    </Frame>
</Ui>